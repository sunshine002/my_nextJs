{
	"extends": "dtslint/dt.json",
	"rules": {
		// TODOs
		"dt-header": false,
		"no-any-union": true,
		"no-empty-interface": false,
		"no-object-literal-type-assertion": false,
		"no-unnecessary-generics": false,
		"no-void-expression": false,
		"strict-export-declare-modifiers": false,
    "void-return": false,
    "semicolon": [
      true,
      "always",
      "ignore-interfaces"
    ],
	}
}
